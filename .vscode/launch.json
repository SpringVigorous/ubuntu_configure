{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python 调试程序: 当前文件",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "F:/test/ubuntu_configure/assist/python/",
      "args": [
        "-i",
        "F:/test/ubuntu_configure/assist/c++/im_export_macro",
        "-o",
        "F:/test/test_c",
        "-r",
        "F:/test/ubuntu_configure/assist/python/integer/replace_folders_json_copy_copy.json",
        "-c",
        "utf-8-sig"
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "f:/test_data",
      "program": "f:/test_data/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}