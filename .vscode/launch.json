{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python 调试程序: 当前文件",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "F:/test/ubuntu_configure/assist/python/",
      "args": [
        "-i",
        "F:/test/co_async/",
        "-c",
        "utf-8-sig",
        "-f",
        ".hpp;.cpp;.h;.cxx;.hxx;.c;.cc;.hh;.inl"
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "f:/test/ubuntu_configure/assist/c++/im_export_macro/src",
      "program": "f:/test/ubuntu_configure/assist/c++/im_export_macro/src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}