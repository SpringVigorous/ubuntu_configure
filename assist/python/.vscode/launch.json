{
  "configurations": [

    {
      "name": "Python 调试程序: 包含参数的当前文件",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "python": "myenv/Scripts/python.exe",
      "justMyCode": false , // 确保这行存在，可以进入系统库代码
      // "args": " -r F:/test  -r F:/test  -p joy_project -m data -f data.student -t"
      "args": "-o F:/test/joy_project/test/test/test_cerel -d F:/test/joy_project/test/test/test_cerel -r assist/replace_params_animal.json",
      "stopOnEntry": false,
      "showReturnValue": true,

    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "f:/test/ubuntu_configure/assist/python/c++/cmake_project/test",
      "program": "f:/test/ubuntu_configure/assist/python/c++/cmake_project/test/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}